      <field name="model">x_crm.progress</field>
      <field name="name">CRM Progress</field>
      <field name="state">manual</field>
      <field name="transient" eval="False"/>
    </record>
    <record model="ir.model" id="model_shipping_request">
      <field name="info">Shipping Requests</field>
      <field name="model">x_crm.shipping.request</field>
